# Server Configuration
PORT=3000

# Database Configuration
# For development with Prisma Postgres (recommended for quick setup)
DATABASE_URL="prisma+postgres://localhost:51213/?api_key=your_prisma_api_key"

# Alternative: Traditional PostgreSQL connection
# DATABASE_URL="postgresql://username:password@localhost:5432/sitescope"

# Environment
NODE_ENV=development

# Logging Configuration
# Available levels: error, warn, info, http, debug
LOG_LEVEL=debug