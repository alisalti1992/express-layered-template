# Docker Environment Variables for SiteScope
# Copy this to .env when running with Docker Compose

NODE_ENV=production
PORT=3000

# PostgreSQL Database (Docker Compose)
DATABASE_URL="postgresql://sitescope:sitescope123@postgres:5432/sitescope?sslmode=disable"

# Redis (for future use)
REDIS_URL="redis://redis:6379"

# Application Settings
LOG_LEVEL=info
API_RATE_LIMIT=100

# Security
SESSION_SECRET=your-super-secret-session-key-change-in-production